<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:cons="https://standaarden.overheid.nl/stop/imop/consolidatie/"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="https://standaarden.overheid.nl/stop/imop/consolidatie/"
           version="1.3.0">
   <xs:annotation>
      <xs:documentation>"consolidatie-schema" van STOP. </xs:documentation>
      <xs:documentation>Dit schema definieert de elementen voor de STOP-modules voor consolidaties</xs:documentation>
      <xs:documentation>versie 1.3.0</xs:documentation>
   </xs:annotation>
   <!--
  ================================================ 
     Geldende regelgeving                               
  ================================================ 
  -->
   <xs:element name="ConsolidatieIdentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:ConsolidatieIdentificatie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="cons:FRBRWork"/>
            <xs:element ref="cons:soortWork"/>
            <xs:element ref="cons:isConsolidatieVan"/>
            <xs:element maxOccurs="1" minOccurs="0" ref="cons:isTijdelijkDeelVan"/>
         </xs:sequence>
         <xs:attributeGroup ref="cons:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="isConsolidatieVan">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:isConsolidatieVan"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="cons:WorkIdentificatie"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="isTijdelijkDeelVan">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:isTijdelijkDeelVan"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="cons:WorkIdentificatie"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="WorkIdentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:WorkIdentificatie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="cons:FRBRWork"/>
            <xs:element ref="cons:soortWork"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="FRBRExpression" type="cons:dtSTOPidentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:FRBRExpression"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="FRBRWork" type="cons:dtSTOPidentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:FRBRWork"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="soortWork" type="cons:dtWaardeRef">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:soortWork"/>
      </xs:annotation>
   </xs:element>
   <!--
  ================================================ 
     Toestand, Proefversie
  ================================================ 
  -->
   <xs:element name="Toestanden">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:Toestanden"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="cons:mgContext"/>
            <xs:choice maxOccurs="unbounded" minOccurs="1">
               <xs:element ref="cons:BekendeToestand"/>
               <xs:element ref="cons:ToestandMetSamenloop"/>
            </xs:choice>
         </xs:sequence>
         <xs:attributeGroup ref="cons:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="BekendeToestand">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:BekendeToestand"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="cons:mgToestandGeneriek"/>
            <xs:group maxOccurs="1" minOccurs="1" ref="cons:mgToestandInhoud"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="ToestandMetSamenloop">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:ToestandMetSamenloop"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="cons:mgToestandGeneriek"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:group name="mgToestandGeneriek">
      <xs:sequence>
         <xs:element maxOccurs="1" minOccurs="1" ref="cons:FRBRExpression"/>
         <xs:element maxOccurs="unbounded" minOccurs="1" ref="cons:gerealiseerdeDoelen"/>
         <xs:element maxOccurs="1" minOccurs="1" ref="cons:geldigheid"/>
      </xs:sequence>
   </xs:group>
   <xs:element name="Proefversies">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:Proefversies"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="cons:mgContext"/>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="cons:Proefversie"/>
         </xs:sequence>
         <xs:attributeGroup ref="cons:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="Proefversie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:Proefversie"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:gerealiseerdeDoelen"/>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:instrumentVersie"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:group name="mgToestandInhoud">
      <xs:sequence>
         <xs:element ref="cons:instrumentVersie"/>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="cons:revisies"/>
      </xs:sequence>
   </xs:group>
   <xs:element name="gerealiseerdeDoelen">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:gerealiseerdeDoelen"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="cons:doel"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="instrumentVersie" type="cons:dtSTOPidentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:instrumentVersie"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="geldigheid">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:geldigheid"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="cons:Geldigheidsperiode"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="Geldigheidsperiode">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:Geldigheidsperiode"/>
      </xs:annotation>
      <xs:complexType>
         <xs:all>
            <xs:element ref="cons:juridischWerkendOp"/>
            <xs:element ref="cons:geldigOp"/>
         </xs:all>
      </xs:complexType>
   </xs:element>
   <xs:element name="juridischWerkendOp">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:juridischWerkendOp"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:Periode"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="geldigOp">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:geldigOp"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="cons:Periode"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="Periode">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:Periode"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:vanaf"/>
            <xs:element maxOccurs="1" minOccurs="0" ref="cons:tot"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="vanaf" type="xs:date">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:vanaf"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="tot" type="xs:date">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:tot"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="revisies">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:revisies"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="cons:revisie"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="revisie" type="cons:dtSTOPidentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:revisie"/>
      </xs:annotation>
   </xs:element>
   <!--
  ================================================ 
     Juridische verantwoording                             
  ================================================ 
  -->
   <xs:element name="JuridischeVerantwoording">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:JuridischeVerantwoording"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="cons:mgContext"/>
            <xs:choice>
               <xs:sequence>
                  <xs:element maxOccurs="1" minOccurs="1" ref="cons:VerantwoordingInstrument"/>
                  <xs:element maxOccurs="unbounded"
                              minOccurs="0"
                              ref="cons:VerantwoordingElement"/>
               </xs:sequence>
               <xs:element maxOccurs="unbounded"
                           minOccurs="1"
                           ref="cons:VerantwoordingElement"/>
            </xs:choice>
         </xs:sequence>
         <xs:attributeGroup ref="cons:agSchemaVersie"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="VerantwoordingInstrument">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:VerantwoordingInstrument"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:heeftVerantwoording"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="VerantwoordingElement">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:VerantwoordingElement"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:wId"/>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:heeftVerantwoording"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="heeftVerantwoording">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:heeftVerantwoording"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="cons:Verantwoording"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="wId" type="xs:token">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:wId"/>
