            <xs:element maxOccurs="1" minOccurs="0" ref="cons:vernietigt"/>
            <xs:element maxOccurs="1" minOccurs="0" ref="cons:vernietigtGedeeltelijk"/>
            <xs:element maxOccurs="1" minOccurs="0" ref="cons:bladwijzers"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="ondertekendOp" type="xs:date">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:ondertekendOp"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="gepubliceerdOp" type="xs:date">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:gepubliceerdOp"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="publicatieIdentifier" type="xs:token">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:publicatieIdentifier"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="publicatienaam" type="xs:token">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:publicatienaam"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="soortGepubliceerdWork" type="cons:dtWaardeRef">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:soortGepubliceerdWork"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="rectificeert" type="cons:dtSTOPidentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:rectificeert"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="vernietigt" type="cons:dtSTOPidentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:vernietigt"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="vernietigtGedeeltelijk" type="cons:dtSTOPidentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:vernietigtGedeeltelijk"/>
      </xs:annotation>
   </xs:element>
