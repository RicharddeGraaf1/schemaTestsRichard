      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="cons:Bladwijzer"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="Bladwijzer">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:Bladwijzer"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="3" minOccurs="1" ref="cons:relevantVoor"/>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:url"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="relevantVoor" type="cons:wlRelevantVoor">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:relevantVoor"/>
      </xs:annotation>
