         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:juridischWerkendVanaf"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="geldigVanaf" type="xs:date">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:geldigVanaf"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="heeftTijdelijkDeel" type="cons:dtSTOPidentificatie">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:heeftTijdelijkDeel"/>
      </xs:annotation>
   </xs:element>
   <xs:element name="gepubliceerdIn">
      <xs:annotation>
         <xs:documentation source="https://koop.gitlab.io/STOP/standaard/1.3.0/go/go.html?id=xsd:cons:gepubliceerdIn"/>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded"
                        minOccurs="1"
                        ref="cons:JuridischeBronpublicatie"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="JuridischeBronpublicatie">
