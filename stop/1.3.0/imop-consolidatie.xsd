      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:url"/>
            <xs:element maxOccurs="1" minOccurs="0" ref="cons:publicatieIdentifier"/>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:publicatienaam"/>
