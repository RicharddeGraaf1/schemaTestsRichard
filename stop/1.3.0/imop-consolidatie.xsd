            <xs:element maxOccurs="1" minOccurs="1" ref="cons:geschorst"/>
            <xs:element maxOccurs="1" minOccurs="1" ref="cons:gepubliceerdIn"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="cons:wId"/>
